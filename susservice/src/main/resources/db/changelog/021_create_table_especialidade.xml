<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="current_timestamp" dbms="postgresql"/>

    <changeSet id="021" author="olciandro.magalhaes">
    
        <createTable tableName="especialidade">
            <column name="seq_especialidade" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false" primaryKeyName="pk_especialidade"/>
            </column>
            
            <column name="descricao" type="varchar(50)" remarks="nome da especialidade medica.">
                <constraints nullable="true" />
            </column>

        </createTable>

    </changeSet>
    
</databaseChangeLog>
